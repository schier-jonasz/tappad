{
    "env": {
        "browser": true,
        "commonjs": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:vue/essential"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "parser": "babel-eslint",
        "ecmaVersion": 2020,
        "sourceType": "module",
        "allowImportExportEverywhere": true
    },
    "plugins": [
        "vue"
    ],
    "rules": {
        "vue/script-setup-uses-vars": "off",
        "no-console": "warn",
        "no-empty": "warn",
        "no-undef": "warn",
        "vue/multi-word-component-names": 0
    }
}